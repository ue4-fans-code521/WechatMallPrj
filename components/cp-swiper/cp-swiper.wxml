<swiper class="swiper" duration="500" autoplay indicator-dots indicator-active-color="#ff6270" interval="2000" circular>
  <block wx:for="{{li}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>

